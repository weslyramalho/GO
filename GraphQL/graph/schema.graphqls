type Category {
  id: ID!
  name: String!
  description: String
  courses: [Couse!]!
}

type Couse {
  id: ID!
  name: String!
  description: String
  category: Category!
}

input NewCategory {
  name:String!
  description : String
  }

input NewCourse  {
  name:String!
  description : String
  categoryId: ID!
  }

type Query {
  categories:[Category]!
  course: [Couse!]!
}

type Mutation {
  createCategory(input: NewCategory!): Category!
  creatCourse(input: NewCourse!): Course!
}